<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".UaActivity">
    <TextView
        android:id="@+id/textview_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text="현재 앱에서 테스트시 사용할 user agent 설정" />
    <EditText
        android:id="@+id/input_ua_config_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>
    <EditText
        android:id="@+id/input_ua_config_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text=""/>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/btn_ua_config_save"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ua save"/>
        <Button
            android:id="@+id/btn_ua_config_close"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ua close"/>
    </LinearLayout>
</LinearLayout>